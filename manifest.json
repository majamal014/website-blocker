{
    "manifest_version": 2,
    "name": "Website Blocker",
    "version": "1.0",
    "permissions": ["storage"],
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Blocker"
    }
}